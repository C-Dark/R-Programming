#4-3
x<-seq(0.4,3,len=11)
y<-10+sin(10*x)
g<-function(x)exp(x)
h<-function(x)-12*x+15
curve(h,from =-2,to=5)
curve(g,from =-2,to=5,add=T)
points(x,y)

