#8-0
f<-function(x,y)(x^2-2*x*y+y^2-3*x)
g<-function(x,y)((x+1)^2+(y-1)^2-1)
x<-seq(-10,10,len=100)
y<-seq(-10,10,len=100)
z<-outer(x,y,f)
w<-outer(x,y,g)
contour(x,y,z,levels=0,col=2)
contour(x,y,w,levels=0,col=3,add=T)

